@model IEnumerable<LearningHub.Models.APITokenTbl>

@{
    ViewBag.Title = "Index";
}

@section Styles {
    <!-- Custom styles for this page -->
    <link href="~/Content/Admin/vendor/datatables/dataTables.bootstrap4.min.css" rel="stylesheet">
}
<div class="card-body">
    <p>
        <div class="row">
            <div class="col-md-4">
                @Html.ActionLink("Create New", "Create", new { area = "" }, new { @class = "btn btn-primary btn-user btn-block" })
            </div>

        </div>
    </p>
    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.APIName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.APIKey)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.SenderID)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.IsActive)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.IsTest)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.IsChannel)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.IsUnicord)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.IsFlash)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EnteredOn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.UpdatedOn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Route)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.APIName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.APIKey)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.SenderID)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsActive)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsTest)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsChannel)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsUnicord)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.IsFlash)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.EnteredOn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.UpdatedOn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Route)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Edit", new { id = item.APITokenID }) |
                            @Html.ActionLink("Details", "Details", new { id = item.APITokenID }) |
                            @Html.ActionLink("Delete", "Delete", new { id = item.APITokenID })
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
@section scripts
{
    <!-- Page level plugins -->
    <script src="~/Content/Admin/vendor/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/Admin/vendor/datatables/dataTables.bootstrap4.min.js"></script>

    <!-- Page level custom scripts -->
    <script src="~/Content/Admin/js/demo/datatables-demo.js"></script>
}
